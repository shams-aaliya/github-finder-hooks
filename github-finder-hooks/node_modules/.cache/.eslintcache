;\n        dispatch({\n            type: GET_REPOS,\n            payload: res.data\n        })\n      }\n      \n\n    // Clear Users\n\n    const clearUsers = () => dispatch({type: CLEAR_USERS});\n\n    // Set Loading\n\n      const setLoading = () => dispatch({type: SET_LOADING});\n\n    \n\n    return (\n\n        <GithubContext.Provider\n        value={{\n            \n            users:state.users,\n            user:state.user,\n            repos:state.repos,\n            loading:state.loading,\n            searchUsers,\n            clearUsers,\n            getUser,\n            getUserRepos\n        }}>\n        {props.children}\n        </GithubContext.Provider>\n        )\n }\n\nexport default GithubState;","/home/shams_aaliya/Coding/Tutorial/github-finder-hooks/src/context/github/githubContext.js",[],"/home/shams_aaliya/Coding/Tutorial/github-finder-hooks/src/context/github/githubReducer.js",["75","76"],"/home/shams_aaliya/Coding/Tutorial/github-finder-hooks/src/context/type.js",[],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":7,"column":1,"nodeType":"83","messageId":"84","endLine":7,"endColumn":10},{"ruleId":"81","severity":1,"message":"82","line":3,"column":5,"nodeType":"83","messageId":"84","endLine":3,"endColumn":14},{"ruleId":"85","severity":1,"message":"86","line":11,"column":1,"nodeType":"87","endLine":50,"endColumn":7},"no-native-reassign",["88"],"no-negated-in-lhs",["89"],"no-unused-vars","'SET_ALERT' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation"]